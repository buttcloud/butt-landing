{
  "name": "butt-landing",
  "version": "1.0.0",
  "description": "a public-facing landing page for your Scuttlebutt pub server",
  "main": "index.js",
  "bin": "bin.js",
  "scripts": {
    "start": "node ./bin",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test": "npm-run-all -s test:lint test:deps",
    "docker:build": "docker build -t buttcloud/butt-landing .",
    "docker:push": "docker push buttcloud/butt-landing",
    "docker:run": "docker run -it --rm --init -v ~/.ssb:/home/node/.ssb --net=host buttcloud/butt-landing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/butt-landing.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/butt-landing/issues"
  },
  "homepage": "https://github.com/ahdinosaur/butt-landing#readme",
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "npm-run-all": "^4.0.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "pino": "^4.16.0",
    "pino-http": "^3.2.1",
    "pull-reconnect": "0.0.3",
    "server-summary": "^6.1.0",
    "ssb-client": "^4.5.7"
  }
}
